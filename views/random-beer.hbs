<h1>Random beer list</h1>

{{#each randomBeer}}
  <img src={{this.image_url}} alt='' />

  <h3>{{this.name}}</h3>
  <dt>{{this.description}}</dt>
  <sub>{{this.tagline}}</sub>

  {{!-- <p>Food Pairing:{{food_pairing}}</p> --}}

  <ul>
    <li>{{food_pairing}}</li>
  </ul>
  <p>Brewer Tips:{{brewers_tips}}</p>

{{/each}}